while True:

    print('Para encerrar, digite "SAIR"\n')
    cidade = input("Digite uma cidade: ")
    cidade = cidade.lower()
    print("")

    if cidade == "continuar": continue
    elif cidade == 'sair': break
    else: print("A cidade digitada foi: "+cidade+ "\n")

print("\nEstou fora do laÃ§o e encerrando")
